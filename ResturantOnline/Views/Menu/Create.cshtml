 @model MenuCreateViewModel


<div class="container">
<h1>Create</h1>
<div class="text-danger" asp-validation-summary="All"></div>
<form method="post" asp-action="Create" asp-controller="Menu" enctype="multipart/form-data">
     <div>
        <label asp-for="Titel"></label>
        <input asp-for="Titel" class="form-control" />
        <span asp-validation-for="Titel" class="text-danger"></span>
    </div>
      <div>
        <label asp-for="Type"></label>
        <input asp-for="Type" class="form-control" />
        <span asp-validation-for="Type" class="text-danger"></span>
    </div>
     <div>
        <label asp-for="Description"></label>
        <input asp-for="Description" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    @*<div>
        <label asp-for="Status"></label>
        <select  asp-for="Status" class="form-control">
            <option value="true">Active</option>
            <option value="false">InActive</option>
         </select>
        <span asp-validation-for="Status" class="text-danger"></span>
    </div>*@
    <div class="form-group form-check">
         <label asp-for="Status" class="form-check-label">Active/InActive</label>
         <input class="form-check-input" asp-for="Status" /> 
         <span asp-validation-for="Status" class="text-danger"></span>
     </div>
     <div>
        <label asp-for="Image"></label>
        <input  asp-for="Image" type="file" class="form-control" />
        <span asp-validation-for="Image" class="text-danger"></span>
     </div>
    
     
      
    <input type="submit" class="btn btn-primary" value="Create" />
</form>
</div>


